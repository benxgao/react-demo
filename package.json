    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "classnames": "2.2.5",
