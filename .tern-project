{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "src/**/*.js"
  ],
  "dontLoad": [
    ".test_cache/**",
    "coverage/**",
    "node_modules/**",
    "public/**"
  ],
  "plugins": {
    "modules": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
